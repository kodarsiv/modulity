{
    "name": "kodarsiv/modulity",
    "version": "2.0.0",
    "description": "A Module Generator CLI Package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "artisan",
        "module",
        "generator"
    ],
    "authors": [
        {
            "name": "Taner Tombas",
            "email": "tombastaner@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Kodarsiv\\Modulity\\": "src/",
            "Modules\\" : "app/Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodarsiv\\Modulity\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodarsiv\\Modulity\\Providers\\ModulityServiceProvider"
            ]
        }
    }
}
